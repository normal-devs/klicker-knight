{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FishingRoom",
  "type": "object",
  "properties": {
    "type": {
      "const": "fishingRoom"
    },
    "playerState": {
      "enum": [
        "AtEntrance",
        "BrokenLine",
        "Caught",
        "Fishing"
      ]
    },
    "brokenRod": {
      "type": "boolean"
    },
    "fishCaught": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["type", "playerState", "brokenRod", "fishCaught"],
  "additionalProperties": false
}
